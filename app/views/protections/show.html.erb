<div class="page-header">
  <%= link_to book_insurance_path(@protection.insurance.book, @protection.insurance), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_insurance_protection_path(@protection.insurance, @protection), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show protection</h1>
</div>

<dl class="dl-horizontal">
  <dt>Year:</dt>
  <dd><%= @protection.year %></dd>

  <dt>Age:</dt>
  <dd><%= @protection.age %></dd>

  <dt>Coverage Rate:</dt>
  <dd><%= @protection.coverage_rate %></dd>

</dl>
