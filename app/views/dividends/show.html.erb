<div class="page-header">
<% if @dividend.insurance.is_master? %>
  <%= link_to master_insurance_path(@dividend.insurance), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
<% else %>
   <%= link_to book_insurance_path(@dividend.insurance.book, @dividend.insurance), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
<% end %>

  <%= link_to edit_insurance_dividend_path(@dividend.insurance, @dividend), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show dividend</h1>
</div>

<dl class="dl-horizontal">
  <dt>Year:</dt>
  <dd><%= @dividend.year %></dd>

  <dt>Age:</dt>
  <dd><%= @dividend.age %></dd>

  <dt>Amount:</dt>
  <dd><%= @dividend.amount %></dd>

</dl>
