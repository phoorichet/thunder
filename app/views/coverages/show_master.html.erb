<div class="page-header">
  <%= link_to masters_coverages_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %> 

  <%= link_to edit_master_coverage_path(@coverage), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show coverage</h1>
</div>

<dl class="dl-horizontal">
  <dt>Key:</dt>
  <dd><%= @coverage.key %></dd>

  <dt>Value:</dt>
  <dd><%= @coverage.value %></dd>

  <dt>Desciption:</dt>
  <dd><%= @coverage.description %></dd>

  <dt>Tag List:</dt>
  <dd><%= @coverage.tag_list %></dd>

</dl>



