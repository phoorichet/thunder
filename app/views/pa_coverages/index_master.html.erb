<div class="page-header">
  <%= link_to new_master_pa_coverage_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Coverage
  <% end %>
  <h1>Listing Master Coverages</h1> 
</div>


<div class="row">
  <div class="col-lg-12">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Key</th>
          <th>Value</th>
          <th>Description</th>
          <th>Tag List</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @pa_coverages.each do |pa_coverage| %>
          <tr>
            <td><%= pa_coverage.key %></td>
            <td><%= pa_coverage.value %></td>
            <td><%= pa_coverage.description %></td>
            <td><%= pa_coverage.tag_list %></td>
            <td><%= link_to 'Show', master_pa_coverage_path(pa_coverage) %></td>
            <td><%= link_to 'Edit', edit_master_pa_coverage_path(pa_coverage) %></td>
            <td><%= link_to 'Destroy', master_pa_coverage_path(pa_coverage), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
</div>  
<div class="row">
  <div class="col-lg-12">
    <div class="text-center">
      <%= paginate @pa_coverages, :theme => 'twitter-bootstrap-3' %>  
    </div>
  </div>
</div>

